		"svelte": "^3.48.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.4",
		"tslib": "^2.4.0",
		"typescript": "~4.7.4"
